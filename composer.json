{
	"name": "juniwalk/sandbox",
	"description": "The sandbox is a pre-packaged Nette Framework project, basic configured structure for your application.",
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],

	"autoload": {
		"psr-4": {"App\\": "src/"},
		"classmap": ["src/Bootstrap.php"]
	},

	"require": {
		"php": ">=8.0",
		"nette/application": "^3.0",
		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.0",
		"nette/di": "^3.0",
		"nette/security": "^3.0",
		"tracy/tracy": "^2.0",
		"nettrine/migrations": "^0.7",
		"nettrine/orm": "^0.8",
        "juniwalk/tessa": "^1.0",
		"juniwalk/form": "^3.0",
		"ublaboo/datagrid": "^6.0",
		"ublaboo/mailing": "^1.0",
		"contributte/translation": "^0.9",
		"contributte/recaptcha": "^3.0",
		"contributte/console": "^0.9",
		"ramsey/uuid": "^3.0",

		"nasext/dependent-select-box": "^5.0"
	},

	"minimum-stability": "dev",
	"prefer-stable": true
}
